    function _createCellTextPaint()
    {
        var paint = new Paint();
        paint.typeface = Typeface.fromName("36 days ago BRK");
        paint.textSize = 2 * this._integers.cellWidth / 3;
        paint.color = 0xffffffff;

        return paint;
    }

    function _loadExplosion()
    {
        //here we load all bitmaps to memory, boxause loading in OnDraw function takes too much time
        var frames = new Array();
        var q = 1;
        var file = "";
        var allImages = 39
        for (var q = 1; q < allImages; q++) {
            file = "app://exp/exp_";
            //name of first files are exp_01, exp_02 ... so we have to add zero befire value in q if q is less then 10
            if (q < 10)
                file = file + "0";
            file = file + q.toString() + ".png";
            frames[q] = Bitmap.fromFile(file);
        }
        return frames;
    }

