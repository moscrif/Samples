    // @region properties
    property vectors(v)
    {
        get return this._vectors;
        set logW("Try to set get only property Resources.vectors!");
    }

    property paints(v)
    {
        get return this._paints;
        set logW("Try to set get only property Resources.vectors!");
    }

    property integers(v)
    {
        get return this._integers;
        set logW("Try to set get only property Resources.integers!");
    }

    property images(v)
    {
        get return this._images;
        set logW("Try to set get only property Resources.images!");
    }
    // @endregion

    // @region private functions
    function _placeVector(x, y, width, height, path)
    {
        var (pathLeft, pathTop, pathRight, pathBottom) = path.getBounds();
    
        // scale graphics
        var matrix = new Matrix();
        var scale = Float.min(width / (pathRight - pathLeft), height / (pathBottom - pathTop));
        matrix.setScale(scale, scale);
        path.transform(matrix);
    
        // move graphics
        (pathLeft, pathTop, pathRight, pathBottom) = path.getBounds();
        path.offset(x - (pathRight - pathLeft) / 2 - pathLeft, y - pathTop - (pathBottom - pathTop)/ 2);

        return path;
    }

    function _createBackgroundPaint()
    {
        var start = {
            x: System.width / 2,
            y: 0
        };
        var end = {
            x: System.width / 2,
            y: System.height
        };
        var pts = {
            start: start;
            end: end;
        };
        var clrs = new Array();
        clrs[0] = 0xff10AAD2;
        clrs[1] = 0xff0E415F;
        
        var pos = new Array();
        pos[0] = 0;
        pos[1] = 1;
        
        var shader = Shader.linear(pts, clrs, pos,0);
        
        var paint = new Paint();
        paint.shader = shader;

        return paint;
    }

    function _createLogoPaint()
    {
        var paint = new Paint();
        paint.color = 0xffffffff;
        return paint;
    }

    function _createCoveredCellPaint()
    {
       //some points are same for every shader
       var start = {
            x: this._integers.cellWidth / 2,
            y: this._integers.cellWidth / -2,
        };
        var end = {
            x: this._integers.cellWidth / -2,
            y: this._integers.cellWidth / 2
        };
        var pts = {
            start: start;
            end: end;
        };

        //shader for non uncover cells
        var clrs = new Array();
        var pos = new Array();

        clrs[0] = 0xffffffff;
        clrs[1] = 0x7fffffff;
        clrs[2] = 0x3fffffff;
        clrs[3] = 0x3fffffff;

        pos[0] = 0;
        pos[1] = 0.25;
        pos[2] = 0.5;
        pos[3] = 1;

        var paint = new Paint();
        paint.shader = Shader.linear(pts, clrs, pos, 0);

        return paint;
    }

    function _createMarkedCellPaint()
    {
       //some points are same for every shader
       var start = {
            x: this._integers.cellWidth / 2,
            y: this._integers.cellWidth / -2,
        };
        var end = {
            x: this._integers.cellWidth / -2,
            y: this._integers.cellWidth / 2,
        };
        var pts = {
            start: start;
            end: end;
        };

        //shader for non uncover cells
        var clrs = new Array();
        var pos = new Array();
        //_marked
        clrs = new Array();
        pos = new Array();

        clrs[0] = 0xffFF2300;
        clrs[1] = 0xffB90000;

        pos[0] = 0;
        pos[1] = 1;

        var paint = new Paint();
        paint.shader = Shader.linear(pts, clrs, pos,0);

